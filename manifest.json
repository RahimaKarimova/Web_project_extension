{
  "manifest_version": 3,
  "name": "Form Auto Filler",
  "version": "1.0",
  "description": "An extension to auto-fill forms and extract data.",
  "permissions": [
    "scripting",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.png"
  },
  "icons": {
    "48": "icons/icon.png"
  }
}